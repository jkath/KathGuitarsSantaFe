<h1>Listing guitar_pics</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
  </tr>

<% @guitar_pics.each do |guitar_pic| %>
  <tr>
    <td><%=h guitar_pic.title %></td>
    <td><%=h guitar_pic.description %></td>
    <td><%=h guitar_pic.photo_file_name %></td>
    <td><%=h guitar_pic.photo_content_type %></td>
    <td><%=h guitar_pic.photo_file_size %></td>
    <td><%= link_to 'Show', guitar_pic %></td>
    <td><%= link_to 'Edit', edit_guitar_pic_path(guitar_pic) %></td>
    <td><%= link_to 'Destroy', guitar_pic, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New guitar_pic', new_guitar_pic_path %>